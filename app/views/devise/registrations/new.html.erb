<h2>Sign up</h2>

<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>

  <div>
    <%= f.label :user_name %>
    <%= f.text_field :user_name, autofocus: true, :placeholder=>"User Name"%>
  </div>
  
  <div>
  <%= f.email_field :email,  :placeholder=>" Your email here" %></div>

  <div><% if @validatable %><%@minimum_password_length %><% end %><br />
    <%= f.password_field :password, autocomplete: "off", :placeholder=> " Password" %></div>

  <div><br />
    <%= f.password_field :password_confirmation, autocomplete: "off", :placeholder=>" Confirm your password" %></div>

  <div><%= f.submit "Sign up", :class=>"btn btn-primary", :style=>"margin-top:10px"%></div>
<% end %>

<%= render "devise/shared/links" %>
